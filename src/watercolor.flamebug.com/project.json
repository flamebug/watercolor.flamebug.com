{
  "version": "0.0.0",
  "dependencies": {
    "Microsoft.AspNet.Antiforgery": "1.0.0-rc1",
    "Microsoft.AspNet.Diagnostics": "1.0.0-rc1",
    "Microsoft.AspNet.Mvc": "6.0.0-rc1",
    "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-rc1",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1",
    "Microsoft.AspNet.Tooling.Razor": "1.0.0-rc1",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-rc1",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc1",
    "Microsoft.Extensions.Configuration.UserSecrets": "1.0.0-rc1",
    "Microsoft.Extensions.Logging": "1.0.0-rc1",
    "Microsoft.Extensions.Logging.Console": "1.0.0-rc1",
    "MailKit": "1.3.0-beta2"
  },
  "commands": {
    "web": "Microsoft.AspNet.Hosting --config hosting.json"
  },
  "frameworks": {
    "dnxcore50": {}
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "scripts": {
    "postrestore": [
      "npm install",
      "bower install"
    ],
    "prepare": [
      "gulp prepare"
    ]
  }
}